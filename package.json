    "@vitejs/plugin-react": "5.0.3",
    "autoprefixer": "10.4.21",
    "gh-pages": "6.3.0",
    "husky": "9.1.7",
    "lint-staged": "16.2.1",
    "postcss": "8.5.6",
    "prettier": "3.6.2",
    "tailwindcss": "4.1.13",
    "ts-node": "10.9.2",
    "typescript": "5.9.2",
    "vite": "7.1.7"
