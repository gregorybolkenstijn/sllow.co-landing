    "@typescript-eslint/parser": "^8.0.0",
    "@vitejs/plugin-react": "4.2.1",
    "autoprefixer": "10.4.19",
    "eslint": "^9.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "gh-pages": "6.1.1",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.1",
    "postcss": "8.4.38",
    "prettier": "3.6.2",
    "tailwindcss": "3.4.4",
    "ts-node": "10.9.2",
    "typescript": "5.2.2",
